{
  "name": "video-management-system",
  "version": "1.0.0",
  "description": "Video Management System with AI Integration using MERN Stack",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "setup": "node setup.js",
    "setup:dev": "npm run setup && npm run dev"
  },
  "keywords": ["video", "ai", "management", "mern", "streaming"],
  "author": "VMS Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.2",
    "multer": "^1.4.5-lts.1",
    "opencv4nodejs": "^5.6.0",
    "node-cron": "^3.0.2",
    "axios": "^1.5.0",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
} 